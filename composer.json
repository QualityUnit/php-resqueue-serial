{
    "type": "project",
    "homepage": "https://github.com/QualityUnit/resqu-server",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "ext-json": "*",
        "colinmollenhour/credis": "^1.9.1",
        "symfony/yaml": "^3.2",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "qualityunit/resqu-client": "~5"
